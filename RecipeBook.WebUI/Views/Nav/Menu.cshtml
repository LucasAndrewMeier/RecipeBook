@model IEnumerable<string>

@Html.ActionLink("All Cuisines", "List", "Recipe", null, new { @class = "menuItems btn-block buttonLogIn btn-med not-selected-this"})

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
   controller = "Recipe",
   action = "List",
   cuisine = link,
   page = 1
},
new { @class = "menuItems btn-block buttonSignUp not-selected-this"+ (link == ViewBag.SelectedCuisine ? "selected-this" : "" )})
}